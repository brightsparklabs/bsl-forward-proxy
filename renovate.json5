{
    enabled: true,
    extends: [
        'config:recommended',
    ],
    timezone: 'Australia/Sydney',
    prHourlyLimit: 25,
    prConcurrentLimit: 25,
    packageRules: [
        {
            groupName: 'All Minor and Patch Updates',
            matchUpdateTypes: [
                'minor',
                'patch',
                'pin',
                'digest',
            ],
            automerge: true,
            automergeType: 'pr',
        },
        {
            groupName: 'Major Updates',
            matchUpdateTypes: [
                'major',
            ],
            automerge: false,
            prCreation: 'immediate',
        },
    ],
}
